# 通用环境变量：会被 .env.development 覆盖。

VITE_APP_NAME=AI Use Declaration
VITE_APP_ENV=development

# 资源打包与路由的基础路径
VITE_PUBLIC_BASE=/
VITE_ROUTER_BASE=/

# API 默认地址
VITE_API_BASE=http://localhost:3000/api

# WebSocket 地址
VITE_WS_URL=

# 功能开关
VITE_FEATURE_EXPORT=true

# dev server 端口（现在是5174）
VITE_DEV_PORT=5174
